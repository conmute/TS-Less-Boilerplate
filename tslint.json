{
  "extends": "tslint:recommended",
  "rules": {
    "interface-name": [],
    "max-line-length": [true, 100],
    "member-ordering": [true,
      "public-before-private",
      "static-before-instance",
      "variables-before-functions"
    ],
    "no-reference": false,
    "no-internal-module": false,
    "no-namespace": false
  }
}
